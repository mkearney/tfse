citHeader("To cite tfse use:")

citEntry(entry        = "PhdThesis",
         title        = "Twitter Follows and Selective Exposure (tfse)",
         author       = as.person("Michael Wayne Kearney"),
         school       = "University of Kansas",
         year         = "2016",
         note         = "R package version 1.0.1",
         url          = "http://github.com/mkearney/tfse",

         textVersion  =
           paste("Kearney, M. W. (2016). Twitter follows and",
                     "selective exposure: Data collection using Twitter's",
                     "REST and streaming APIs. R package version 1.0.1.",
                     "Unpublished PhdThesis in Communication Studies",
                     "at the University of Kansas.", sep = "\n")
)
