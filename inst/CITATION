
citHeader("To cite tfse use:")

citEntry(
    entry        = "Manual",
    title        = "tfse: The First Scriptorium Emporium",
    author       = as.person("Michael W. Kearney"),
    year         = 2017,
    note         = "R package version 0.2.21",
    url          = "https://github.com/mkearney/tfse/",
    key          = "tfse-package",
    textVersion  = paste(
        "Kearney, M. W. (2017). tfse: The First Scriptorium Emporium.",
        "R package version 0.2.21 Retrieved from",
        "https://github.com/mkearney/tfse/"
    )
)
